{
  "expo": {
    "name": "Secure Stop",
    "slug": "secure-stop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "securestop",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/splash-image.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f0f1a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.securestop.app"
    },
    "android": {
      "package": "com.securestop.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#0f0f1a"
      }
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "8a61ec78-7d4b-4676-9fa2-8148ab4f4063"
      }
    }
  }
}
